<h3>Here are all the CDs in this artist library:</h3>
<ol>
@foreach (var cd in @Model["cds"])
{
  <li><a href='/library/@Model["library"].GetId()/cds/@cd.GetId()'>@cd.GetTitle()</a></li>
}
</ol>
<p><a href='/library/@Model["library"].GetId()/cds/new'>Add a new Artist Name</a></p>
<p><a href='/library'>Return to library list</a></p>
<p><a href='/'>Return to Main Page</a></p>
